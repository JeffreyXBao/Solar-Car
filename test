import RPi.GPIO as GPIO
import time
import threading

GPIO.setmode(GPIO.BOARD)
GPIO.setup(11, GPIO.IN, pull_up_down=GPIO.PUD_DOWN)

def callbackTest(channel):
	print "test"

GPIO.add_event_detect(11, GPIO.RISING, 	callback=callbackTest)

try:
	while True:
		    time.sleep(50000.05)
		
except KeyboardInterrupt:
	GPIO.cleanup()
GPIO.cleanup()
